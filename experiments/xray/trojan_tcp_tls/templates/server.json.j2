{
    "log": {
        "loglevel": "debug",
        "access": "/root/access.log",
        "error": "/root/error.log"
    },
    "inbounds": [
        {
            "port": 1234,
            "protocol": "trojan",
            "settings": {
                "clients": [
                    {
                        "password":"securepassword123",
                        "email": "love@example.com"
                    }
                ]
            },
            "streamSettings": {
                "network": "tcp",
                "security": "tls",
                "tlsSettings": {
                    "serverName": "proxy.labforensic.de",
                    "alpn": [
                        "h2",
