config setup
    charondebug="ike 2, knl 2, cfg 2, net 2, esp 2, dmn 2, manager 2"

conn %default
    ikelifetime=60m
    keylife=20m
    rekeymargin=3m
    keyingtries=1
    authby=secret
    keyexchange=ikev2
    ike=aes256-sha256-modp2048
    esp=aes256-sha256

conn myvpn
    left=%any
    leftid={{ relay }}
    leftsubnet=0.0.0.0/0
    right=%any
    rightid=%any
    rightsourceip=10.10.10.0/24
    rightdns=8.8.8.8,1.1.1.1
    auto=add
    compress=no
    type=tunnel
    fragmentation=yes
    leftfirewall=yes